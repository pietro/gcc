# { dg-options "-fstropping=upper" }  #
BEGIN INT i = 10;
      LONG INT ii = LONG 10, LONG LONG INT iii = LONG LONG 10;
      SHORT INT ss = SHORT 10, SHORT SHORT INT sss = SHORT SHORT 10;
      ASSERT (-i = -10);
      ASSERT (-ii  = - LONG 10);
      ASSERT (-iii = - LONG LONG 10);
      ASSERT (-ss  = - SHORT 10);
      ASSERT (-sss = - SHORT SHORT 10)
END
