# { dg-options "-fstropping=upper" }  #
# ELIF with ELSE #
BEGIN
   INT i = 12;
   IF i > 20 THEN
      1
   ELIF i > 12 THEN
      BEGIN
         IF FALSE THEN
            ASSERT (FALSE);
            1
         ELSE
            ASSERT (FALSE);
            1
         FI
      END
   ELIF i < 10 THEN
      ASSERT (FALSE);
      1
   ELSE
      0
   FI
END
