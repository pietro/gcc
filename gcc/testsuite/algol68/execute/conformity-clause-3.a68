# { dg-options "-fstropping=upper" }  #
BEGIN MODE DATUM = UNION(INT,REAL,CHAR);
      DATUM datum := 20;
      INT i = CASE INT i = 10; datum
              IN (REAL): 2,
                 (INT val): val + i
              OUT INT x = 100;
                  x + 10
              ESAC;
      ASSERT (i = 30)
END
