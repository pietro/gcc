# { dg-options "-fstropping=upper" }  #
# Using the factorial function in a program #
BEGIN PROC f = (INT n) INT:
      BEGIN INT product := 1;
            FOR i TO n DO product *:= i OD;
            product
      END;
      ASSERT (f(0) = 1);
      ASSERT (f(1) = 1);
      ASSERT (f(2) = 2);
      ASSERT (f(3) = 6)
END
