# { dg-options "-fstropping=upper" }  #
# Nested conditional clauses  #
BEGIN
   INT i = 12;
   IF i > 5 THEN
      IF i < 15 THEN
         IF i > 11 THEN
            0
         ELSE
            ASSERT (FALSE);
            1
         FI
      FI
   ELSE
      IF i > 100 THEN
         ASSERT (FALSE);
         1
      ELSE
         ASSERT (FALSE);
         1
      FI
   FI
END
