# { dg-options "-fstropping=upper" }  #
BEGIN INT i = 12;
      LONG INT ii = LONG 12, LONG LONG INT iii = LONG LONG 12;
      SHORT INT s = SHORT 12, SHORT SHORT INT ss = SHORT SHORT 12;
      ASSERT (13 /= i);
      ASSERT (ii NE LONG 13);
      ASSERT (iii /= LONG LONG 13);
      ASSERT (s /= SHORT 13);
      ASSERT (ss /= SHORT SHORT 13)
END
