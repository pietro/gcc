# { dg-options "-fstropping=upper" }  #
# Simple jumps.  #
(INT j:= 0, i;
 k: i:= j;
 IF i >= 2 THEN GOTO l FI;
 puts ("0");
 m: IF i >= 1 THEN n FI;
 puts ("0");
 o: GOTO p;
 l: puts ("1"); i := i - 2; m;
 n: puts ("1"); o;
 p: puts ("");
 j := j + 1;
 IF j <= 3 THEN k FI)
CO
Result:
  0
  0

  0
  1

  1
  0

  1
  1
CO
