# { dg-options "-fstropping=upper" }  #
BEGIN REAL pie, my small real := 0.001;
      PROC my sqrt = (REAL r) REAL: r;
      BEGIN REAL w := 0, INT i := 1, REAL z = my sqrt (my small real / 2);
      loop: w := w + 2 / (i * (i + 2));
            i := i + 4;
            IF 1/i > z THEN GO TO loop FI;
            pie := 4 * w
      END
END
