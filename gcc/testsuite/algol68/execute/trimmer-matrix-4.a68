# { dg-options "-fstropping=upper" }  #
BEGIN [,]INT matrix = ((1,2,3),
                       (4,5,6),
                       (7,8,9));
      []INT column = matrix[1:3,2];
      ASSERT (LWB column = 1);
      ASSERT (UPB column = 3);
      ASSERT (column[1] = 2 AND column[2] = 5 AND column[3] = 8)
END
